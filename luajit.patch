+++ LuaJIT-2.0.4-vita/src/lj_mcode.c	2016-08-06 21:44:14.593271170 +0100
@@ -32,6 +32,10 @@
 void sys_icache_invalidate(void *start, size_t len);
 #endif
 
+#if LJ_TARGET_PSVITA
+#include <psp2/kernel/sysmem.h>
+#endif
+
 /* Synchronize data/instruction cache. */
 void lj_mcode_sync(void *start, void *end)
 {
@@ -44,6 +48,13 @@
   sys_icache_invalidate(start, (char *)end-(char *)start);
 #elif LJ_TARGET_PPC
   lj_vm_cachesync(start, end);
+#elif LJ_TARGET_PSVITA
+  SceUID block = sceKernelFindMemBlockByAddr(start, (char *)end-(char *)start);
+  if(block < 0)
+  {
+    return;
+  }
+  sceKernelSyncVMDomain(block, start, (char *)end-(char *)start);
 #elif defined(__GNUC__)
   __clear_cache(start, end);
 #else
@@ -118,12 +129,70 @@
   return mprotect(p, sz, prot);
 }
 
+#elif LJ_TARGET_PSVITA
+
+#define MCPROT_RW   0
+#define MCPROT_RX   1
+#define MCPROT_RWX  2
+
+static void *mcode_alloc_at(jit_State *J, uintptr_t hint, size_t sz, int prot)
+{
+    (void)hint;
+	(void)prot;
+
+    void *base;
+    SceUID block = sceKernelAllocMemBlockForVM("code", sz);
+	if(block < 0)
+	{
+		lj_trace_err(J, LJ_TRERR_MCODEAL);
+		return NULL;
+	}
+
+    if(sceKernelGetMemBlockBase(block, &base) < 0)
+	{
+		lj_trace_err(J, LJ_TRERR_MCODEAL);
+		return NULL;
+	}
+
+    return base;
+}
+
+static void mcode_free(jit_State *J, void *p, size_t sz)
+{
+	SceUID block = sceKernelFindMemBlockByAddr(p, sz);
+	if(block < 0)
+	{
+		return;
+	}
+	sceKernelFreeMemBlock(block);
+}
+
+
+static int mcode_setprot(void *p, size_t sz, int prot)
+{
+	(void)p;
+	(void)sz;
+	if(prot == MCPROT_RW)
+	{
+		sceKernelOpenVMDomain();
+	}
+	else if(prot == MCPROT_RX)
+	{
+		sceKernelCloseVMDomain();
+	}
+	else
+	{
+		return -1;
+	}
+	return 0;
+}
+
+
 #elif LJ_64
 
 #error "Missing OS support for explicit placement of executable memory"
 
 #else
-
